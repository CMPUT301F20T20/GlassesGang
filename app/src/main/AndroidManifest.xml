<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.glassesgang">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BookProfileActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GoogleSignInActivity" />
        <activity android:name=".HomeActivity"
            android:exported="true"></activity>  <!--Added this so I can launch HomeActivity without having to launch Main Activity-->
        <activity android:name=".AddBookActivity"></activity>
        <activity android:name=".EditBookActivity"></activity>
    </application>

</manifest>